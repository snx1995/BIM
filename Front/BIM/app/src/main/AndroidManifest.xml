<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="top.banyaoqiang.www.bim">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppMaterialDesign">
        <activity android:name=".LauncherActivity"
            android:theme="@style/LauncherActivityTheme"/>
        <activity android:name=".MainActivity" />
        <activity android:name=".MessageActivity" />
        <activity android:name=".AppTestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddressBookActivity" />
        <activity
            android:name=".InfoDetailActivity"
            android:theme="@style/SystemStatusBarTransparent" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".LoginActivity"
            android:theme="@style/LoginActivityTheme"
            >

        </activity>
    </application>

</manifest>